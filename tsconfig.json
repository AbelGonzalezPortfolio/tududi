{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "moduleResolution": "node", // Ensures TypeScript resolves modules like 'react-router-dom'
    "jsx": "react",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["app/frontend/**/*"]
}
